{
  "name": "neo4j-browser-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "run-p health-check build",
    "build": "vite build",
    "dev": "vite",
    "test:watch": "vitest --coverage --silent",
    "health-check": "run-p lint type-check",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "clean": "run-p clean:dist clean:node_modules",
    "clean:dist": "rm -rf node_modules",
    "clean:node-modules": "rm -rf node_modules"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.9.6",
    "@vueuse/core": "^10.1.2",
    "lodash": "^4.17.21",
    "neo4j-driver": "^5.8.1",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.3",
    "vee-validate": "^4.9.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
